{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@tensorflow-models/face-landmarks-detection": ["types/face-landmarks-detection"]
    },
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ]
  }
}